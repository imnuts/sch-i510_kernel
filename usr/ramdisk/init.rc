	disabled
	oneshot

service wlandutservice /system/bin/wlandutservice
	disabled
	oneshot

service macloader /system/bin/macloader
	oneshot
# end of wifi

service dhcpcd /system/bin/dhcpcd
	disabled
	oneshot

service iprenew_eth0 /system/bin/dhcpcd -n
